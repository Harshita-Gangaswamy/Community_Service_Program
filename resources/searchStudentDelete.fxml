		xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" 
		fx:controller="commTracker.SearchStudentDeleteController">
	<children>
		
		<Text strokeWidth="10.0" style="-fx-font-size: 40; -fx-fill: black" 
		text="Enter Student ID to Delete" textAlignment="CENTER" layoutX = "150.0" layoutY = "170"/>
		
